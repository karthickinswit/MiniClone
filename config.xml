<?xml version='1.0' encoding='utf-8'?>
<widget id="com.appiyo.matrix_mini_marketclone" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MatrixMiniMarket</name>
    <description>
    </description>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
    	<preference name="amatrixClonendroid-minSdkVersion" value="26" />
        <preference name="android-targetSdkVersion" value="29" />
        <preference name="AndroidXEnabled" value="true" />
        <allow-intent href="market:*" />
        

	<icon src="res/android/icon.png"/>
    	<splash src="res/android/screen.png"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
